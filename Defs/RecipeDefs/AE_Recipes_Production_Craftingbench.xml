<Defs>
	<RecipeDef>
    <defName>Make_NanoMachinery</defName>
    <label>make nanomachinery</label>
    <description>Make some nanomachinery.</description>
    <jobString>Making nanomachinery.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>4000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
	<recipeUsers>
	<li>BasicArchotechWorkbench</li>
	</recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ComponentIndustrial</li>
        <li>Silver</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Nanomachinery>5</Nanomachinery>
    </products>
    <researchPrerequisite>ArchotechMimicry</researchPrerequisite>
    <skillRequirements>
      <Crafting>15</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>
  
